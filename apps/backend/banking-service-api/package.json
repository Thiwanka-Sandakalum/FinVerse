{
    "name": "banking-service-api",
    "version": "1.0.0",
    "description": "API for managing financial products, including creation, editing, bookmarking, comparing, sharing, and rating products",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\"",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:studio": "prisma studio",
        "prisma:seed": "ts-node prisma/seed.ts",
        "test": "jest"
    },
    "keywords": [
        "banking",
        "financial-products",
        "api",
        "express",
        "typescript",
        "prisma"
    ],
    "author": "FinVerse API Team",
    "license": "ISC",
    "dependencies": {
        "@clerk/express": "^1.7.16",
        "@prisma/client": "",
        "axios": "^1.11.0",
        "cors": "",
        "express": "",
        "helmet": "",
        "jsonwebtoken": "",
        "morgan": "",
        "uuid": "^11.1.0",
        "winston": ""
    },
    "devDependencies": {
        "@types/cors": "",
        "@types/express": "",
        "@types/jest": "",
        "@types/jsonwebtoken": "",
        "@types/morgan": "",
        "@types/node": "",
        "@types/supertest": "",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "",
        "@typescript-eslint/parser": "",
        "eslint": "",
        "eslint-config-prettier": "",
        "jest": "",
        "prettier": "",
        "prisma": "",
        "supertest": "",
        "ts-jest": "",
        "ts-node-dev": "",
        "typescript": ""
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}
