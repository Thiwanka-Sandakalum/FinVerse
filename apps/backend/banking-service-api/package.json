{
    "name": "banking-service-api",
    "version": "1.0.0",
    "description": "API for managing financial products, including creation, editing, bookmarking, comparing, sharing, and rating products",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\"",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:studio": "prisma studio",
        "prisma:seed": "ts-node prisma/seed.ts",
        "test": "jest"
    },
    "keywords": [
        "banking",
        "financial-products",
        "api",
        "express",
        "typescript",
        "prisma"
    ],
    "author": "FinVerse API Team",
    "license": "ISC",
    "dependencies": {
        "@clerk/express": "^1.7.17",
        "@prisma/client": "^6.13.0",
        "@types/amqplib": "^0.10.7",
        "amqplib": "^0.10.8",
        "axios": "^1.11.0",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "prisma": "^6.13.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.1",
        "uuid": "^11.1.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.2.1",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "eslint": "^9.33.0",
        "eslint-config-prettier": "^10.1.8",
        "jest": "^30.0.5",
        "prettier": "^3.6.2",
        "prisma": "^6.13.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.1",
        "ts-node-dev": "^2.0.0"
    }
}